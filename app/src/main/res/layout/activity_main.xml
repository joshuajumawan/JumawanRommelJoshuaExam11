<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="109dp"
        android:layout_height="24dp"
        android:text="Practical Exam 1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005" />

    <TextView
        android:id="@+id/textView"
        style="@style/Desc1"
        android:layout_width="138dp"
        android:layout_height="43dp"
        android:text="NBA.COM"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="27dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="335dp"
        android:layout_height="63dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="LOS ANGELES LAKERS"
        style="@style/Desc1"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="214dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="430dp"
        android:layout_height="144dp"
        app:srcCompat="@drawable/m2"
        tools:layout_editor_absoluteX="-43dp"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="178dp"
        android:layout_height="123dp"
        android:background="#38006b"
        android:text="With all this energy around and attention on the Lakers’ excitement, the team has only one choice: tunnel vision on what LeBron calls “championship habits every day.” - Lebron James"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="298dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="127dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/m4"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="298dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lebron James"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="462dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brandon Ingram"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player Profiles"
        style="@style/Desc1"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:text="Lonzo Ball"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="510dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="146dp"
        android:layout_height="wrap_content"
        android:text="Rajon Rondo"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="510dp" />

</android.support.constraint.ConstraintLayout>